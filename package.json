{
  "name": "sky",
  "version": "1.0.0",
  "description": "Sky project for simple ReactJS applications using flux.",
  "main": "js/App.js",
  "scripts": {
    "start": "webpack-dev-server -d",
    "build": "webpack",
    "lint": "eslint . --ext .js --ext .jsx --cache -f node_modules/eslint-html-reporter/reporter.js -o lint/report.html",
    "test": "mocha --compilers js:babel-core/register",
    "doc": "./node_modules/.bin/jsdoc -c conf.json -t ./node_modules/ink-docstrap/template -R README.md -r -d docs"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "keywords": [
    "reactjs",
    "flux",
    "eslint",
    "jsdoc",
    "es6"
  ],
  "author": "Kushal D'Souza",
  "license": "ISC",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "axios": "^0.11.0",
    "babel-eslint": "^6.0.4",
    "babel-jest": "^12.0.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-html-reporter": "^0.5.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "events": "^1.1.0",
    "expect": "^1.18.0",
    "expect-jsx": "^2.5.1",
    "flux": "^2.1.1",
    "ink-docstrap": "^1.1.4",
    "jest-cli": "^12.0.2",
    "jsdoc": "^3.4.0",
    "keymirror": "^0.1.1",
    "mocha": "^2.4.5",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.0.2",
    "react-router": "^2.4.0"
  }
}
